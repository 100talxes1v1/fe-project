{
  "name": "boston-app-<%=appName%>",
  "version": "0.1.1",
  "description": "<%=description%>",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bpm debug",
    "build": "bpm build",
    "deploy": "bpm publish",
    "mock": "cross-env IS_MOCK=true npm run dev",
    "lint:js": "eslint --ext .js,.vue,.html --fix --format node_modules/eslint-friendly-formatter src",
    "lint:css": "stylelint src/**/*.vue,scss,css --fix",
    "commit": "git-cz",
    "analyze": "source-map-explorer 'dist/**/*.js'"
  },
  "author": "<%=author%>",
  "dependencies": {
    "@xes/dh-boston-sync-state": "^0.1.4",
    "@xes/dh-boston-type": "^0.4.2",
    "@xes/dh-component-vue-common-table": "^0.5.5",
    "@xes/dh-guardian-sdk": "^0.3.8",
    "@xes/dh-module-env": "^0.2.1",
    "@xes/dh-module-getter": "^0.1.6",
    "@xes/dh-module-product-line": "^0.2.6",
    "@xes/dh-module-request": "^0.1.10",
    "@xes/dh-module-url": "^0.1.4",
    "@xes/dh-sensor": "^2.1.8",
    "@xes/fe-mp-business-components": "^0.1.22",
    "element-ui": "^2.13.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1",
    "vuex-map-fields": "^1.4.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "@xes/dh-boston-package-manager": "^0.4.11",
    "@xes/dh-commitlint-config": "0.0.2",
    "@xes/eslint-config-dahai": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.24.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "moment": "^2.27.0",
    "prettier": "^2.0.5",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.18.0",
    "vue-loader": "^15.9.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bostonDependencies": {
    "element-runtime@0.1.1": [
      "element-ui",
      "element-ui/lib/theme-chalk/index.css"
    ],
    "statistics-runtime@0.1.1": [
      "@xes/dh-guardian-sdk",
      "@xes/dh-sensor"
    ],
    "vue-runtime@0.1.1": [
      "vue",
      "vue-class-component",
      "vue-property-decorator",
      "vue-router",
      "vuex"
    ]
  },
  "hooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,vue,json,css,scss,html}": [
      "prettier --write"
    ],
    "src/**/*.{js,vue,html}": [
      "npm run lint:js",
      "npm run lint:css"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo $GIT_PARAMS",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
