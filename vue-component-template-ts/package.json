    "predo": "npm run lint",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development node build/dev.js",
    "build": "cross-env NODE_ENV=production node build/prod.js",
    "karma": "cross-env BABEL_ENV=test karma start ./test/karma.conf.js",
    "release": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "<%=author%>",
